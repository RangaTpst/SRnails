{

    
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "Core\\": "core/",
        "Tests\\Factories\\": "tests/factories/",
        "Tests\\": "tests/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^12.0",
      "phpunit/phpcov": "^11.0",
        "phpdocumentor/phpdocumentor": "^2.8"
    },
    "require": {
        "phpdocumentor/shim": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
